{{ define "content" }}
{{ if . }}
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">URL</th>
    </tr>
  </thead>
  <tbody>
  {{ range $i, $c := . }}
    <tr>
      <th scope="row">{{ $i }}</th>
      <td>{{ $c.ID }}</td>
      <td>{{ $c.Name }}</td>
      <td><a href="{{ $c.URL }}">{{ $c.URL }}</a></td>
    </tr>
  {{ end }}
  </tbody>
</table>
{{ end }}
{{ end }}